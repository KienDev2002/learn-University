#include<stdio.h>
#include<math.h>
#include<conio.h>
#define size 100
void nhapin (int n , float a[],float b[], int m){
	int i ;
	for (i=0 ; i<n ; i++){
		printf ("a[%d]=",i);
		scanf ("%f",&a[i]);
	}
	for (i=0 ; i<n ; i++){
		printf ("%0.2f\t",a[i]);
	}
		for (i=0 ; i<m ; i++){
		printf ("\nB[%d]=",i);
		scanf ("%f",&b[i]);
	}
	for (i=0 ; i<m ; i++){
		printf ("%0.2f\t",b[i]);
	}
}
int main(){
   int n,m;
   float a[size];
   float b[size];
   	printf ("nhap n =");
   	scanf ("%d",&n);
   	printf ("nhap m =");
   	scanf ("%d",&m);
   	nhapin(n,a,b,m);
   	return 0;
}
